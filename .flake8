[flake8]
ignore = E231, E241, W503, E402
per-file-ignores =
    __init__.py:F401
max-line-length = 120
exclude =
    .git,
    __pycache__,
    build,
    dist,
count = true
extend-ignore =
    # See https://github.com/PyCQA/pycodestyle/issues/373
    E203,
